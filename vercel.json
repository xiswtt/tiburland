{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/article/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/articles",
      "destination": "/server.js"
    },
    {
      "source": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "source": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "source": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "source": "/favicon/(.*)",
      "dest": "/favicon/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ]
}